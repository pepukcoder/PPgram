<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Design.PreviewWith>
		<Border Padding="10" Background="#000000">
			<StackPanel Orientation="Vertical">
				<StackPanel Orientation="Horizontal" Margin="0,5">
					<Border Background="{StaticResource BackgroundWindow}" Width="50" Height="50" CornerRadius="10" Margin="5,0"/>
					<Border Background="{StaticResource Surface}" Width="50" Height="50" CornerRadius="10" Margin="5,0"/>
					<Border Background="{StaticResource SurfaceLight}" Width="50" Height="50" CornerRadius="10" Margin="5,0"/>
					<Border Background="{StaticResource Panel}" Width="50" Height="50" CornerRadius="10" Margin="5,0"/>
					<Border Background="{StaticResource PanelLight}" Width="50" Height="50" CornerRadius="10" Margin="5,0"/>
					<Border Background="{StaticResource PanelAccent}" Width="50" Height="50" CornerRadius="10" Margin="5,0"/>
				</StackPanel>
				<StackPanel Orientation="Horizontal" Margin="0,5">
					<Border Background="{StaticResource PPgramAccent}" Width="50" Height="50" CornerRadius="10" Margin="5,0"/>
					<Border Background="{StaticResource PPgramAccentLight}" Width="50" Height="50" CornerRadius="10" Margin="5,0"/>
				</StackPanel>
				<StackPanel Orientation="Horizontal" Margin="0,5">
					<Border Background="{StaticResource PrimaryText}" Width="50" Height="50" CornerRadius="10" Margin="5,0"/>
					<Border Background="{StaticResource SecondaryText}" Width="50" Height="50" CornerRadius="10" Margin="5,0"/>
					<Border Background="{StaticResource Selection}" Width="50" Height="50" CornerRadius="10" Margin="5,0"/>
				</StackPanel>
			</StackPanel>
		</Border>
	</Design.PreviewWith>
	
	
	<SolidColorBrush x:Key="BackgroundWindow">#121212</SolidColorBrush>
	<SolidColorBrush x:Key="Surface">#1e2021</SolidColorBrush>
	<SolidColorBrush x:Key="SurfaceLight">#242627</SolidColorBrush>
	<SolidColorBrush x:Key="Panel">#353637</SolidColorBrush>
	<SolidColorBrush x:Key="PanelLight">#494a4b</SolidColorBrush>
	<SolidColorBrush x:Key="PanelAccent">#808080</SolidColorBrush>

	<SolidColorBrush x:Key="SelectionList" Opacity="0.1" Color="#353637"></SolidColorBrush>
	<SolidColorBrush x:Key="BackgroundSemi" Opacity="0.8" Color="#121212"></SolidColorBrush>

	<SolidColorBrush x:Key="PPgramAccent">#3f86e7</SolidColorBrush>
	<SolidColorBrush x:Key="PPgramAccentLight">#5292e9</SolidColorBrush>
	
	<SolidColorBrush x:Key="PrimaryText">#ffffff</SolidColorBrush>
	<SolidColorBrush x:Key="SecondaryText">#808081</SolidColorBrush>
	<SolidColorBrush x:Key="Selection">#868687</SolidColorBrush>
	
	<SolidColorBrush x:Key="ButtonIdle">#606060</SolidColorBrush>
	<SolidColorBrush x:Key="ButtonHover">#808080</SolidColorBrush>
</ResourceDictionary>
